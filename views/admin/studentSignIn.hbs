<!-- PAGE CONTAINER-->
<div class="page-container2" style="overflow: scroll">
    <!-- MAIN CONTENT -->
    <div class="main-content">
        {{!-- <div class="section__content section__content--p30"> --}}
        {{!-- <div class="container-fluid"> --}}
        {{!-- <div class="row"> --}}

        <div class="container">
            {{>default/messages}}
            {{!-- table here --}}
            <h3>STUDENTS SIGNED IN</h3>
            <form action="/admin/deleteAllStudentsSignIn" method="POST">
                <button class="btn btn-warning float-right mb-1" type="submit">CLEAR</button>
            </form>
            <table class="table table-bordered">
                <thead class="bg-dark text-center">
                    <tr class="text-white">
                        <th>Date</th>
                        <th>Name</th>
                        <th>Student ID</th>
                        
                    </tr>
                </thead>
                <tbody class="text-center">
                    {{#each studentsIn}}
                    <tr>
                        <td>{{date}}</td>
                        <td>{{fullName}}</td>
                        <td>{{studentId}}</td>
                        
                    </tr>
                    {{/each}}

                </tbody>
            </table>
            {{!-- table Ends --}}
        </div>